<?xml version="1.0"?>
<odoo>
    <record id="dha_document_project_list" model="ir.ui.view">
        <field name="name">dha.document.project.form</field>
        <field name="model">ir.attachment</field>
        <field name="priority">30</field>
        <field name="arch" type="xml">
            <form create="false">
                <sheet>
                    <group>
                        <field name="project_ids" nolabel="1"/>
                    </group>
                </sheet>
                <footer>
                    <button name="attach_project" type="object" string="Save" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>